{
    "name": "ogan/skeleton",
    "description": "Ogan Skeleton - Projet de dÃ©marrage pour Ogan Framework",
    "type": "project",
    "keywords": [
        "framework",
        "mvc",
        "php",
        "skeleton",
        "starter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Schmitt Sylvain",
            "email": "sylvain.schmitt@delnyx.fr"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ogan/core": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"copy('.env.example', '.env');\"",
            "@php bin/console tailwind:init",
            "@php bin/console assets:install"
        ]
    }
}